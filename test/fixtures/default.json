{
  "version": "2",
  "networks": {
    "n1": {},
    "n2": null
  },
  "volumes": {
    "v1": {},
    "v2": null
  },
  "services": {
    "s1": {
      "build": "./s1",
      "ports": [
        "80:5000"
      ],
      "depends_on": [
        "s3"
      ],
      "environment": [
        "SOME_VAR=some=value"
      ],
      "env_file": "./relative/parent1/parent2/parent3/../../twoupwards.env"
    },
    "s2": {
      "build": "./s2",
      "depends_on": [
        "s1",
        "s3"
      ],
      "privileged": true,
      "environment": {
        "SOME_VAR": "some value"
      },
      "env_file": [
        "./relative/parent1/parent2/parent3/../../twoupwards.env",
        "./relative/parent1/twoupwards.env",
        "./foo/bar/env.env"
      ],
      "extra_hosts": {
        "foo": "127.0.0.1"
      }
    },
    "s3": {
      "image": "some/image",
      "ports": [
        1000,
        "1001:1002",
        "1003:1004/tcp"
      ],
      "extra_hosts": [
        "bar:8.8.8.8"
      ]
    }
  }
}
